<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <font
        android:font="@font/cambay_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/cambay_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <font
        android:font="@font/cambay_regular_italic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/cambay_regular_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />

    <font
        android:font="@font/cambay_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/cambay_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <font
        android:font="@font/cambay_bold_italic"
        android:fontStyle="italic"
        android:fontWeight="700"
        app:font="@font/cambay_bold_italic"
        app:fontStyle="italic"
        app:fontWeight="700" />
</font-family>